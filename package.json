{
  "name": "wdk-failover-provider",
  "version": "1.0.0",
  "type": "module",
  "description": "The zero-dependency failover provider",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node src --clean --format esm,cjs --dts --sourcemap --out-dir dist",
    "test": "npm run build && noba --register tsx ./tests/*.test.ts",
    "build:test": "tsup-node tests --clean --silent --format esm --out-dir dist/tests",
    "test:node": "npm run build:test && noba ./dist/tests/*.test.js",
    "test:bare": "npm run build:test && noba-bare ./dist/tests/*.test.js"
  },
  "keywords": [],
  "author": "Tu Phan",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.0.9",
    "bare-node-runtime": "^1.1.4",
    "ethers": "^6.16.0",
    "noba": "^1.6.4",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
